# Copy this file as terraform.tfvars and customize the values
# instance and space can be found in the URL for the space you want to use
# https://your_instance.atlassian.net/wiki/spaces/your_space/...

instance = "your_instance"            # part of the URL before "atlassian.net"
user     = "me@example.com"           # your Confluence user's email address
token    = "XXXXXXXXXXXXXXXXXXXXXXXX" # create tokens here: https://id.atlassian.com/manage/api-tokens
space    = "your_space"               # part of the URL after "/wiki/spaces/"
